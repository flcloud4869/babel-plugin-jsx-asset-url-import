{
  "name": "babel-plugin-jsx-asset-url-import",
  "type": "module",
  "version": "1.0.1",
  "description": "Extract and transform static url to import in JSX",
  "scripts": {
    "build": "tsc && tsup",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky install"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "author": "suiyun39 <suiyun39@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/suiyun39/babel-plugin-jsx-asset-url-import#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suiyun39/babel-plugin-jsx-asset-url-import.git"
  },
  "bugs": {
    "url": "https://github.com/suiyun39/babel-plugin-jsx-asset-url-import/issues"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "jsx"
  ],
  "packageManager": "pnpm@8.9.2",
  "dependencies": {
    "srcset-parse": "^1.1.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/preset-typescript": "^7.23.2",
    "@types/babel__core": "^7.20.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.18.6",
    "@types/react": "^18.2.31",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-plugin-tester": "^11.0.4",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
